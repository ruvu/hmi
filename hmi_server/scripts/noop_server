#!/usr/bin/env python

'''
A HMI server that always responds with an empty result
'''

import rospy
from hmi_msgs.msg import HumanInteractionAction, HumanInteractionResult, Result
from actionlib import SimpleActionServer
from hmi_server.abstract_client import AbstractHMIServer

class HMIServer(AbstractHMIServer):
    def _determine_answer(self, question, spec, choices):
        return HumanInteractionResult()

if __name__ == '__main__':
	rospy.init_node('noop_server')
	HMIServer(rospy.get_name())
	rospy.spin()